Cell["\<\
Before computing expected values in unit tests, we first compute the third \
order kernel\
\>", "Text",
 CellChangeTimes->{{3.909222502871273*^9, 3.909222504615303*^9}, {
  3.9092225386471367`*^9, 3.909222565628723*^9}, {3.909222655894113*^9, 
  3.909222669543292*^9}},ExpressionUUID->"a312cc2a-faa4-4c82-aea3-\
159fe06f2846"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"WTilde", "[", "q_", "]"}], ":=", 
   RowBox[{"Piecewise", "[", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{
          FractionBox["1", "6"], 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"2", "-", 
             RowBox[{"RealAbs", "[", "q", "]"}]}], ")"}], "3"]}], "-", 
         RowBox[{
          FractionBox["2", "3"], 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"1", "-", 
             RowBox[{"RealAbs", "[", "q", "]"}]}], ")"}], "3"]}]}], ",", 
        RowBox[{
         RowBox[{"RealAbs", "[", "q", "]"}], "<=", "1"}]}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         FractionBox["1", "6"], 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"2", "-", 
            RowBox[{"RealAbs", "[", "q", "]"}]}], ")"}], "3"]}], ",", 
        RowBox[{"1", "<", 
         RowBox[{"RealAbs", "[", "q", "]"}], "<=", "2"}]}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"0", ",", 
        RowBox[{
         RowBox[{"RealAbs", "[", "q", "]"}], ">", "2"}]}], "}"}]}], "}"}], 
    "]"}]}], ";"}]], "Input",
 CellLabel->"In[25]:=",ExpressionUUID->"dabfb2fa-b432-4ad2-9c81-9d13bcf1a263"],

Cell["And we compute the normalizations", "Text",
 CellChangeTimes->{{3.9092225978177013`*^9, 3.9092226043136797`*^9}, {
  3.909222635312488*^9, 3.90922263612142*^9}, {3.909222687852889*^9, 
  3.909222689773716*^9}},ExpressionUUID->"57e5fc52-3b20-4e6b-b6b3-\
17b49ed473b0"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"N1", "=", 
   RowBox[{"Integrate", "[", 
    RowBox[{
     RowBox[{"WTilde", "[", "q", "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"q", ",", 
       RowBox[{"-", "2"}], ",", "2"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"N2", "=", 
   RowBox[{"Integrate", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"WTilde", "[", "r", "]"}], "r"}], ",", 
     RowBox[{"{", 
      RowBox[{"r", ",", "0", ",", "2"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"\[Phi]", ",", "0", ",", 
       RowBox[{"2", "Pi"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"N3", "=", 
   RowBox[{"Integrate", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"WTilde", "[", "r", "]"}], 
      SuperscriptBox["r", "2"], 
      RowBox[{"Sin", "[", "\[Theta]", "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"r", ",", "0", ",", "2"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"\[Phi]", ",", "0", ",", 
       RowBox[{"2", "Pi"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"\[Theta]", ",", "0", ",", "\[Pi]"}], "}"}]}], "]"}]}], 
  ";"}]}], "Input",
 CellChangeTimes->{{3.909222697421171*^9, 3.9092227011220503`*^9}},
 CellLabel->"In[26]:=",ExpressionUUID->"86a6ca36-10e2-4f96-9fee-67d467f589d5"],

Cell["We define then the 1D, 2D and 3D kernels", "Text",
 CellChangeTimes->{
  3.909222745862433*^9, {3.909222797673976*^9, 3.909222811791065*^9}, {
   3.909222855169291*^9, 
   3.9092228699276876`*^9}},ExpressionUUID->"7f1b8225-ab4b-478f-a349-\
afd0e94cb090"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"W1", "[", 
   RowBox[{"r_", ",", "h_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{
    RowBox[{"WTilde", "[", 
     RowBox[{"r", "/", "h"}], "]"}], "/", "N1"}], "/", 
   "h"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"W2", "[", 
   RowBox[{"r_", ",", "h_"}], "]"}], ":=", 
